schema models_profile {
    document models_profile {
        # E.g. "primaries", "experimentals"
        field id type string {
            indexing: attribute | summary | index
        }
        # TODO: Add name, to be used to be included on documents and passages
        
        # E.g. <Q990, n2nuerdn>
        field concepts_versions type map<string, string> {
            indexing: summary

            struct-field key {
                indexing: attribute | summary
            }
            struct-field value {
                indexing: attribute | summary
            }
        }
    }
}
